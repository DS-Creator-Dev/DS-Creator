{
  "name": "ds-creator",
  "version": "1.0.0",
  "description": "DS Creator is a new game engine that let's you create games for the Nintendo DS.",
  "main": "./src/electron/main.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && electron .",
    "build-win": "electron-packager . DS-Creator --overwrite --asar=true --platform=win32 --arch=ia32 --icon=images/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"DS Creator\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BowersIndustry/DS-Creator.git"
  },
  "keywords": [],
  "author": "Kenyon Bowers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BowersIndustry/DS-Creator/issues"
  },
  "homepage": "https://github.com/BowersIndustry/DS-Creator#readme",
  "dependencies": {
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "electron-store": "^8.0.1",
    "electron": "^18.1.0",
    "electron-packager": "^15.5.1"
  }
}
